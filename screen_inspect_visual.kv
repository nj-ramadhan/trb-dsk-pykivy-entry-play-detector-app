						halign: "center"
						valign: "middle"
						font_style: 'H6'

					MDCard:
						orientation: "horizontal"
						padding: 10
						on_press: root.open_sub_body()
						MDLabel:
							text: "Body, Pintu dan Kaca"
							halign: "left"
							valign: "middle"
							font_style: 'H6'

						MDIconButton:
							id: bt_body
